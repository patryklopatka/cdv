tekst =  "Anna , Pawel , RomEk"
tab = tekst.split(",")
print(tekst)
print(tab)
imie1=tab[2]
print(imie1)
print("twoje imie: " +imie1)
imieDuze = imie1.upper()
print (imieDuze)

imiemale = imie1.lower()
print (imiemale)

#sprawdzamie zawartosci
zawartosc=imie1.isalpha()
print(type(zawartosc))

imie = ""
zawartosc = imie1.isalpha()
print(zawartosc)
print(type(zawartosc))

